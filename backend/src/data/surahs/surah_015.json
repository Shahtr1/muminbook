    "surahStart": false
  },
  {
    "uuid": 1822,
    "sno": 20,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَجَعَلْنَا لَكُمْ فِيهَا مَعَٰيِشَ وَمَن لَّسْتُمْ لَهُۥ بِرَٰزِقِينَ",
    "audioUrl": "015020",
