    "surahStart": false
  },
  {
    "uuid": 1818,
    "sno": 16,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَلَقَدْ جَعَلْنَا فِى ٱلسَّمَآءِ بُرُوجًۭا وَزَيَّنَّٰهَا لِلنَّٰظِرِينَ",
    "audioUrl": "015016",
    "manzil": 3,
