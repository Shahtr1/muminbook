    "surahStart": false
  },
  {
    "uuid": 1842,
    "sno": 40,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِلَّا عِبَادَكَ مِنْهُمُ ٱلْمُخْلَصِينَ",
    "audioUrl": "015040",
    "manzil": 3,
    "ruku": 220,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
