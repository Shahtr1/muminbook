    "surahStart": false
  },
  {
    "uuid": 1832,
    "sno": 30,
    "surahId": 15,
    "juzId": 14,
    "ayat": "فَسَجَدَ ٱلْمَلَٰٓئِكَةُ كُلُّهُمْ أَجْمَعُونَ",
    "audioUrl": "015030",
