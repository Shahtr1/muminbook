    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1869,
    "sno": 67,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَجَآءَ أَهْلُ ٱلْمَدِينَةِ يَسْتَبْشِرُونَ",
