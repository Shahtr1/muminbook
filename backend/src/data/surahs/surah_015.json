    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1860,
    "sno": 58,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالُوٓا۟ إِنَّآ أُرْسِلْنَآ إِلَىٰ قَوْمٍۢ مُّجْرِمِينَ",
    "audioUrl": "015058",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1861,
    "sno": 59,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِلَّآ ءَالَ لُوطٍ إِنَّا لَمُنَجُّوهُمْ أَجْمَعِينَ",
    "audioUrl": "015059",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1862,
    "sno": 60,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِلَّا ٱمْرَأَتَهُۥ قَدَّرْنَآ ۙ إِنَّهَا لَمِنَ ٱلْغَٰبِرِينَ",
    "audioUrl": "015060",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1863,
    "sno": 61,
    "surahId": 15,
    "juzId": 14,
    "ayat": "فَلَمَّا جَآءَ ءَالَ لُوطٍ ٱلْمُرْسَلُونَ",
    "audioUrl": "015061",
    "manzil": 3,
    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1864,
    "sno": 62,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالَ إِنَّكُمْ قَوْمٌۭ مُّنكَرُونَ",
    "audioUrl": "015062",
    "manzil": 3,
    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1865,
    "sno": 63,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالُوا۟ بَلْ جِئْنَٰكَ بِمَا كَانُوا۟ فِيهِ يَمْتَرُونَ",
    "audioUrl": "015063",
    "manzil": 3,
    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1866,
    "sno": 64,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَأَتَيْنَٰكَ بِٱلْحَقِّ وَإِنَّا لَصَٰدِقُونَ",
    "audioUrl": "015064",
    "manzil": 3,
    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1867,
    "sno": 65,
    "surahId": 15,
    "juzId": 14,
    "ayat": "فَأَسْرِ بِأَهْلِكَ بِقِطْعٍۢ مِّنَ ٱلَّيْلِ وَٱتَّبِعْ أَدْبَٰرَهُمْ وَلَا يَلْتَفِتْ مِنكُمْ أَحَدٌۭ وَٱمْضُوا۟ حَيْثُ تُؤْمَرُونَ",
