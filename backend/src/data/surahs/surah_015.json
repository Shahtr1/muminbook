    "surahStart": false
  },
  {
    "uuid": 1819,
    "sno": 17,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَحَفِظْنَٰهَا مِن كُلِّ شَيْطَٰنٍۢ رَّجِيمٍ",
