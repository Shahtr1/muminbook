    "surahStart": false
  },
  {
    "uuid": 1843,
    "sno": 41,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالَ هَٰذَا صِرَٰطٌ عَلَىَّ مُسْتَقِيمٌ",
    "audioUrl": "015041",
    "manzil": 3,
    "ruku": 220,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1844,
    "sno": 42,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِنَّ عِبَادِى لَيْسَ لَكَ عَلَيْهِمْ سُلْطَٰنٌ إِلَّا مَنِ ٱتَّبَعَكَ مِنَ ٱلْغَاوِينَ",
    "audioUrl": "015042",
    "manzil": 3,
    "ruku": 220,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1845,
    "sno": 43,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَإِنَّ جَهَنَّمَ لَمَوْعِدُهُمْ أَجْمَعِينَ",
    "audioUrl": "015043",
    "manzil": 3,
    "ruku": 220,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1846,
    "sno": 44,
    "surahId": 15,
    "juzId": 14,
    "ayat": "لَهَا سَبْعَةُ أَبْوَٰبٍۢ لِّكُلِّ بَابٍۢ مِّنْهُمْ جُزْءٌۭ مَّقْسُومٌ",
    "audioUrl": "015044",
    "manzil": 3,
    "ruku": 220,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1847,
    "sno": 45,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّٰتٍۢ وَعُيُونٍ",
    "audioUrl": "015045",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1848,
    "sno": 46,
    "surahId": 15,
    "juzId": 14,
    "ayat": "ٱدْخُلُوهَا بِسَلَٰمٍ ءَامِنِينَ",
    "audioUrl": "015046",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1849,
    "sno": 47,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَنَزَعْنَا مَا فِى صُدُورِهِم مِّنْ غِلٍّ إِخْوَٰنًا عَلَىٰ سُرُرٍۢ مُّتَقَٰبِلِينَ",
    "audioUrl": "015047",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1850,
    "sno": 48,
    "surahId": 15,
    "juzId": 14,
    "ayat": "لَا يَمَسُّهُمْ فِيهَا نَصَبٌۭ وَمَا هُم مِّنْهَا بِمُخْرَجِينَ",
    "audioUrl": "015048",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1851,
    "sno": 49,
    "surahId": 15,
    "juzId": 14,
    "ayat": "۞ نَبِّئْ عِبَادِىٓ أَنِّىٓ أَنَا ٱلْغَفُورُ ٱلرَّحِيمُ",
    "audioUrl": "015049",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1852,
    "sno": 50,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَأَنَّ عَذَابِى هُوَ ٱلْعَذَابُ ٱلْأَلِيمُ",
    "audioUrl": "015050",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1853,
    "sno": 51,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَنَبِّئْهُمْ عَن ضَيْفِ إِبْرَٰهِيمَ",
    "audioUrl": "015051",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1854,
    "sno": 52,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِذْ دَخَلُوا۟ عَلَيْهِ فَقَالُوا۟ سَلَٰمًۭا قَالَ إِنَّا مِنكُمْ وَجِلُونَ",
    "audioUrl": "015052",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1855,
    "sno": 53,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالُوا۟ لَا تَوْجَلْ إِنَّا نُبَشِّرُكَ بِغُلَٰمٍ عَلِيمٍۢ",
    "audioUrl": "015053",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1856,
    "sno": 54,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالَ أَبَشَّرْتُمُونِى عَلَىٰٓ أَن مَّسَّنِىَ ٱلْكِبَرُ فَبِمَ تُبَشِّرُونَ",
    "audioUrl": "015054",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1857,
    "sno": 55,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالُوا۟ بَشَّرْنَٰكَ بِٱلْحَقِّ فَلَا تَكُن مِّنَ ٱلْقَٰنِطِينَ",
    "audioUrl": "015055",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1858,
    "sno": 56,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالَ وَمَن يَقْنَطُ مِن رَّحْمَةِ رَبِّهِۦٓ إِلَّا ٱلضَّآلُّونَ",
    "audioUrl": "015056",
    "manzil": 3,
    "ruku": 221,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1859,
    "sno": 57,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالَ فَمَا خَطْبُكُمْ أَيُّهَا ٱلْمُرْسَلُونَ",
