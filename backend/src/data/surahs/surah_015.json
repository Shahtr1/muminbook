    "surahStart": false
  },
  {
    "uuid": 1875,
    "sno": 73,
    "surahId": 15,
    "juzId": 14,
    "ayat": "فَأَخَذَتْهُمُ ٱلصَّيْحَةُ مُشْرِقِينَ",
