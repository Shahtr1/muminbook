    "surahStart": false
  },
  {
    "uuid": 1823,
    "sno": 21,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَإِن مِّن شَىْءٍ إِلَّا عِندَنَا خَزَآئِنُهُۥ وَمَا نُنَزِّلُهُۥٓ إِلَّا بِقَدَرٍۢ مَّعْلُومٍۢ",
    "audioUrl": "015021",
    "manzil": 3,
    "ruku": 219,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
