  {
    "uuid": 1833,
    "sno": 31,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِلَّآ إِبْلِيسَ أَبَىٰٓ أَن يَكُونَ مَعَ ٱلسَّٰجِدِينَ",
    "audioUrl": "015031",
