    "surahStart": false
  },
  {
    "uuid": 1829,
    "sno": 27,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَٱلْجَآنَّ خَلَقْنَٰهُ مِن قَبْلُ مِن نَّارِ ٱلسَّمُومِ",
    "audioUrl": "015027",
    "manzil": 3,
