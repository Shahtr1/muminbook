    "surahStart": false
  },
  {
    "uuid": 1873,
    "sno": 71,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالَ هَٰٓؤُلَآءِ بَنَاتِىٓ إِن كُنتُمْ فَٰعِلِينَ",
    "audioUrl": "015071",
    "manzil": 3,
    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1874,
    "sno": 72,
    "surahId": 15,
    "juzId": 14,
