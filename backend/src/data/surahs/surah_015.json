    "surahStart": false
  },
  {
    "uuid": 1827,
    "sno": 25,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَإِنَّ رَبَّكَ هُوَ يَحْشُرُهُمْ ۚ إِنَّهُۥ حَكِيمٌ عَلِيمٌۭ",
    "audioUrl": "015025",
    "manzil": 3,
    "ruku": 219,
    "hizbQuarter": 105,
