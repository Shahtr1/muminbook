    "surahStart": false
  },
  {
    "uuid": 1825,
    "sno": 23,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَإِنَّا لَنَحْنُ نُحْىِۦ وَنُمِيتُ وَنَحْنُ ٱلْوَٰرِثُونَ",
    "audioUrl": "015023",
