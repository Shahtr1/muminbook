  },
  {
    "uuid": 1820,
    "sno": 18,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِلَّا مَنِ ٱسْتَرَقَ ٱلسَّمْعَ فَأَتْبَعَهُۥ شِهَابٌۭ مُّبِينٌۭ",
