    },
    "surahStart": false
  },
  {
    "uuid": 1807,
    "sno": 5,
    "surahId": 15,
    "juzId": 14,
    "ayat": "مَّا تَسْبِقُ مِنْ أُمَّةٍ أَجَلَهَا وَمَا يَسْتَـْٔخِرُونَ",
    "audioUrl": "015005",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1808,
    "sno": 6,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَقَالُوا۟ يَٰٓأَيُّهَا ٱلَّذِى نُزِّلَ عَلَيْهِ ٱلذِّكْرُ إِنَّكَ لَمَجْنُونٌۭ",
    "audioUrl": "015006",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1809,
    "sno": 7,
    "surahId": 15,
    "juzId": 14,
    "ayat": "لَّوْ مَا تَأْتِينَا بِٱلْمَلَٰٓئِكَةِ إِن كُنتَ مِنَ ٱلصَّٰدِقِينَ",
    "audioUrl": "015007",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1810,
    "sno": 8,
    "surahId": 15,
    "juzId": 14,
    "ayat": "مَا نُنَزِّلُ ٱلْمَلَٰٓئِكَةَ إِلَّا بِٱلْحَقِّ وَمَا كَانُوٓا۟ إِذًۭا مُّنظَرِينَ",
    "audioUrl": "015008",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1811,
    "sno": 9,
    "surahId": 15,
    "juzId": 14,
    "ayat": "إِنَّا نَحْنُ نَزَّلْنَا ٱلذِّكْرَ وَإِنَّا لَهُۥ لَحَٰفِظُونَ",
    "audioUrl": "015009",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1812,
    "sno": 10,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَلَقَدْ أَرْسَلْنَا مِن قَبْلِكَ فِى شِيَعِ ٱلْأَوَّلِينَ",
    "audioUrl": "015010",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1813,
    "sno": 11,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَمَا يَأْتِيهِم مِّن رَّسُولٍ إِلَّا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ",
    "audioUrl": "015011",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1814,
    "sno": 12,
    "surahId": 15,
    "juzId": 14,
    "ayat": "كَذَٰلِكَ نَسْلُكُهُۥ فِى قُلُوبِ ٱلْمُجْرِمِينَ",
    "audioUrl": "015012",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1815,
    "sno": 13,
    "surahId": 15,
    "juzId": 14,
    "ayat": "لَا يُؤْمِنُونَ بِهِۦ ۖ وَقَدْ خَلَتْ سُنَّةُ ٱلْأَوَّلِينَ",
    "audioUrl": "015013",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1816,
    "sno": 14,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَلَوْ فَتَحْنَا عَلَيْهِم بَابًۭا مِّنَ ٱلسَّمَآءِ فَظَلُّوا۟ فِيهِ يَعْرُجُونَ",
    "audioUrl": "015014",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1817,
    "sno": 15,
    "surahId": 15,
    "juzId": 14,
    "ayat": "لَقَالُوٓا۟ إِنَّمَا سُكِّرَتْ أَبْصَٰرُنَا بَلْ نَحْنُ قَوْمٌۭ مَّسْحُورُونَ",
    "audioUrl": "015015",
    "manzil": 3,
    "ruku": 218,
    "hizbQuarter": 105,
    "sajda": {
