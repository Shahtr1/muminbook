    "surahStart": false
  },
  {
    "uuid": 1830,
    "sno": 28,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَإِذْ قَالَ رَبُّكَ لِلْمَلَٰٓئِكَةِ إِنِّى خَٰلِقٌۢ بَشَرًۭا مِّن صَلْصَٰلٍۢ مِّنْ حَمَإٍۢ مَّسْنُونٍۢ",
    "audioUrl": "015028",
    "manzil": 3,
    "ruku": 220,
    "hizbQuarter": 105,
    "sajda": {
      "recommended": false,
