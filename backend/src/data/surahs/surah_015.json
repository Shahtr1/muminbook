    "audioUrl": "015069",
    "manzil": 3,
    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1872,
    "sno": 70,
    "surahId": 15,
    "juzId": 14,
    "ayat": "قَالُوٓا۟ أَوَلَمْ نَنْهَكَ عَنِ ٱلْعَٰلَمِينَ",
