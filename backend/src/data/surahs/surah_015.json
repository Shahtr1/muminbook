    "manzil": 3,
    "ruku": 222,
    "hizbQuarter": 106,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 1868,
    "sno": 66,
    "surahId": 15,
    "juzId": 14,
    "ayat": "وَقَضَيْنَآ إِلَيْهِ ذَٰلِكَ ٱلْأَمْرَ أَنَّ دَابِرَ هَٰٓؤُلَآءِ مَقْطُوعٌۭ مُّصْبِحِينَ",
