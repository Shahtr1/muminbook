    "surahStart": false
  },
  {
    "uuid": 3735,
    "sno": 30,
    "surahId": 36,
    "juzId": 23,
    "ayat": "يَٰحَسْرَةً عَلَى ٱلْعِبَادِ ۚ مَا يَأْتِيهِم مِّن رَّسُولٍ إِلَّا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ",
    "audioUrl": "036030",
    "manzil": 5,
    "ruku": 382,
    "hizbQuarter": 177,
    "sajda": {
      "recommended": false,
      "obligatory": false
    },
    "surahStart": false
  },
  {
    "uuid": 3736,
    "sno": 31,
    "surahId": 36,
    "juzId": 23,
    "ayat": "أَلَمْ يَرَوْا۟ كَمْ أَهْلَكْنَا قَبْلَهُم مِّنَ ٱلْقُرُونِ أَنَّهُمْ إِلَيْهِمْ لَا يَرْجِعُونَ",
    "audioUrl": "036031",
    "manzil": 5,
    "ruku": 382,
