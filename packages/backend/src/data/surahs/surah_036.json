  },
  {
    "uuid": 3726,
    "sno": 21,
    "surahId": 36,
    "juzId": 22,
    "ayat": "ٱتَّبِعُوا۟ مَن لَّا يَسْـَٔلُكُمْ أَجْرًۭا وَهُم مُّهْتَدُونَ",
