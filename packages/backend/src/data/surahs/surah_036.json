    "surahStart": false
  },
  {
    "uuid": 3722,
    "sno": 17,
    "surahId": 36,
    "juzId": 22,
    "ayat": "وَمَا عَلَيْنَآ إِلَّا ٱلْبَلَٰغُ ٱلْمُبِينُ",
    "audioUrl": "036017",
    "manzil": 5,
    "ruku": 382,
    "hizbQuarter": 176,
    "sajda": {
      "recommended": false,
