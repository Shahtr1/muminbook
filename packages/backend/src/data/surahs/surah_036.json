    "surahStart": false
  },
  {
    "uuid": 3737,
    "sno": 32,
    "surahId": 36,
    "juzId": 23,
    "ayat": "وَإِن كُلٌّۭ لَّمَّا جَمِيعٌۭ لَّدَيْنَا مُحْضَرُونَ",
    "audioUrl": "036032",
    "manzil": 5,
