    "surahStart": false
  },
  {
    "uuid": 3732,
    "sno": 27,
    "surahId": 36,
    "juzId": 22,
    "ayat": "بِمَا غَفَرَ لِى رَبِّى وَجَعَلَنِى مِنَ ٱلْمُكْرَمِينَ",
    "audioUrl": "036027",
    "manzil": 5,
