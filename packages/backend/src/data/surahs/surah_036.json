  },
  {
    "uuid": 3734,
    "sno": 29,
    "surahId": 36,
    "juzId": 23,
    "ayat": "إِن كَانَتْ إِلَّا صَيْحَةًۭ وَٰحِدَةًۭ فَإِذَا هُمْ خَٰمِدُونَ",
    "audioUrl": "036029",
    "manzil": 5,
